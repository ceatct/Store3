<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#E6E6EF"
    android:backgroundTint="#E6E6EF"
    tools:context=".Profile">


    <GridLayout
        android:layout_width="match_parent"
        android:layout_height="100dp"
        android:background="#FFFFFF"
        android:backgroundTint="#FFFFFF">

        <ImageView
            android:id="@+id/imageView2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_row="0"
            android:layout_column="0"
            android:layout_gravity="center"
            app:srcCompat="@drawable/search_bg" />

        <ImageButton
            android:id="@+id/search_bt"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_row="0"
            android:layout_column="0"
            android:backgroundTint="#00FFFFFF"
            android:translationX="40dp"
            android:translationY="20dp"
            app:srcCompat="@drawable/ic_search"
            tools:ignore="SpeakableTextPresentCheck" />

        <TextView
            android:id="@+id/textView2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_row="0"
            android:layout_column="0"
            android:text="Ща шукаєте?"
            android:textColor="#545469"
            android:textSize="16sp"
            android:translationX="100dp"
            android:translationY="40dp" />

        <ImageButton
            android:id="@+id/ok_bt"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_row="0"
            android:layout_column="0"
            android:backgroundTint="#00FFFFFF"
            android:translationX="300dp"
            android:translationY="20dp"
            app:srcCompat="@drawable/ic_ok"
            tools:ignore="SpeakableTextPresentCheck" />
    </GridLayout>

    <GridLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:translationY="125dp">

        <ImageView
            android:id="@+id/imageView3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:translationX="20dp"
            android:translationY="10dp"
            app:srcCompat="@drawable/ic_pro" />

        <TextView
            android:id="@+id/textView8"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Про магазин"
            android:textColor="#545469"
            android:textSize="16sp"
            android:translationX="45dp"
            android:translationY="17dp" />

        <ImageButton
            android:id="@+id/inf_bt"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:backgroundTint="#00FFFFFF"
            android:translationX="175dp"
            app:srcCompat="@drawable/ic_tuda"
            tools:ignore="SpeakableTextPresentCheck" />

        <ImageView
            android:id="@+id/imageView4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_row="1"
            android:layout_column="0"
            android:translationX="20dp"
            android:translationY="10dp"
            app:srcCompat="@drawable/ic_site" />

        <TextView
            android:id="@+id/textView9"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_row="1"
            android:layout_column="1"
            android:text="Перейти на сайт"
            android:textColor="#545469"
            android:textSize="16sp"
            android:translationX="45dp"
            android:translationY="17dp" />

        <ImageButton
            android:id="@+id/site_bt"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_row="1"
            android:layout_column="2"
            android:backgroundTint="#00FFFFFF"
            android:translationX="175dp"
            app:srcCompat="@drawable/ic_tuda"
            tools:ignore="SpeakableTextPresentCheck" />

        <ImageView
            android:id="@+id/imageView6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_row="2"
            android:layout_column="0"
            android:translationX="20dp"
            android:translationY="5dp"
            app:srcCompat="@drawable/ic_alo" />

        <ImageButton
            android:id="@+id/alo_bt"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_row="2"
            android:layout_column="2"
            android:backgroundTint="#00FFFFFF"
            android:translationX="175dp"
            app:srcCompat="@drawable/ic_tuda"
            tools:ignore="SpeakableTextPresentCheck" />

        <TextView
            android:id="@+id/textView10"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_row="2"
            android:layout_column="1"
            android:text="Подзвонити"
            android:textColor="#545469"
            android:textSize="16sp"
            android:translationX="45dp"
            android:translationY="17dp" />

    </GridLayout>

    <GridLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:translationY="325dp">


        <TextView
            android:id="@+id/textView11"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="0 800 123 45 67"
            android:textColor="#545469"
            android:textSize="23sp"
            android:translationX="25dp" />

        <TextView
            android:id="@+id/textView12"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_row="1"
            android:layout_column="0"
            android:text="info@shop.com.ua"
            android:textColor="#545469"
            android:textSize="23sp"
            android:translationX="25dp" />

        <TextView
            android:id="@+id/textView13"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_row="2"
            android:layout_column="0"
            android:text="м. Вінниця, вул. Зодчих, 16"
            android:textColor="#545469"
            android:textSize="23sp"
            android:translationX="25dp" />
    </GridLayout>

    <GridLayout
        android:layout_width="match_parent"
        android:layout_height="180dp"
        android:background="@drawable/faab_panel"
        android:translationY="450dp">


        <TextView
            android:id="@+id/textView14"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Оплата та доставка"
            android:textColor="#3D3E52"
            android:textSize="16sp"
            android:translationX="25dp"
            android:translationY="15dp" />

        <TextView
            android:id="@+id/textView15"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_row="1"
            android:layout_column="0"
            android:text="Оферта"
            android:textColor="#3D3E52"
            android:textSize="16sp"
            android:translationX="25dp"
            android:translationY="25dp" />

        <TextView
            android:id="@+id/cont_txt"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_row="2"
            android:layout_column="0"
            android:text="Контакти"
            android:textColor="#3D3E52"
            android:textSize="16sp"
            android:translationX="25dp"
            android:translationY="35dp" />
    </GridLayout>

    <GridLayout
        android:id="@+id/fab_panel"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_gravity="bottom"
        android:background="@drawable/faab_panel">

        <ImageButton
            android:id="@+id/market_bt"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:backgroundTint="#00FFFFFF"
            android:translationX="10dp"
            app:srcCompat="@drawable/ic_market"
            tools:ignore="SpeakableTextPresentCheck" />

        <ImageButton
            android:id="@+id/like_bt"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:backgroundTint="#00FFFFFF"
            android:translationX="35dp"
            app:srcCompat="@drawable/ic_like"
            tools:ignore="SpeakableTextPresentCheck" />

        <ImageButton
            android:id="@+id/box_bt"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:backgroundTint="#00FFFFFF"
            android:translationX="70dp"
            app:srcCompat="@drawable/ic_box"
            tools:ignore="SpeakableTextPresentCheck" />

        <ImageButton
            android:id="@+id/tri_bt"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:backgroundTint="#00FFFFFF"
            android:translationX="105dp"
            app:srcCompat="@drawable/ic_tri"
            tools:ignore="SpeakableTextPresentCheck" />

        <TextView
            android:id="@+id/textView3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_row="1"
            android:layout_column="0"
            android:text="Головна"
            android:textColor="#545469"
            android:translationX="20dp" />

        <TextView
            android:id="@+id/textView4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_row="1"
            android:layout_column="1"
            android:text="Обране"
            android:textColor="#545469"
            android:translationX="45dp" />

        <TextView
            android:id="@+id/textView5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_row="1"
            android:layout_column="2"
            android:text="Кошик"
            android:textColor="#545469"
            android:translationX="85dp" />

        <TextView
            android:id="@+id/textView6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_row="1"
            android:layout_column="3"
            android:text="Меню"
            android:textColor="#545469"
            android:translationX="120dp" />
    </GridLayout>

</FrameLayout>